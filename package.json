{
  "name": "egg-prometheus",
  "version": "1.7.0",
  "description": "Prometheus plugin for Egg.js",
  "main": "app.js",
  "eggPlugin": {
    "name": "prometheus",
    "optionalDependencies": [
      "rpc"
    ]
  },
  "files": [
    "app.js",
    "lib",
    "app",
    "config",
    "agent.js",
    "index.d.ts"
  ],
  "scripts": {
    "contributor": "git-contributor",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "sh test/init.sh && TEST_TIMEOUT=10000 egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg-prometheus.git"
  },
  "keywords": [
    "egg",
    "prometheus",
    "metrics"
  ],
  "author": "gxcsoccer <gxcsoccer@126.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-prometheus#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "await-first": "^1.0.0",
    "cluster-client": "^3.0.1",
    "connection": "^1.0.0",
    "prom-client": "^11.5.0",
    "sdk-base": "^3.6.0"
  },
  "devDependencies": {
    "await-event": "^2.1.0",
    "colors": "^1.4.0",
    "egg": "^3.16.0",
    "egg-bin": "^6.4.0",
    "egg-mock": "^5.10.6",
    "egg-rpc-base": "^1.3.1",
    "egg-rpc-generator": "^1.3.1",
    "eslint": "^8.56.0",
    "eslint-config-egg": "^12.3.1",
    "git-contributor": "^2.1.5",
    "pedding": "^1.1.0",
    "urllib": "^2.34.0"
  }
}
