{
  "name": "egg-prometheus",
  "version": "1.0.2",
  "description": "Prometheus plugin for Egg.js",
  "main": "app.js",
  "eggPlugin": {
    "name": "prometheus",
    "optionalDependencies": [
      "rpc"
    ]
  },
  "files": [
    "config",
    "lib",
    "app.js"
  ],
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "sh test/init.sh && egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run autod -- --check && egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg-prometheus.git"
  },
  "keywords": [
    "egg",
    "prometheus",
    "metrics"
  ],
  "author": "gxcsoccer <gxcsoccer@126.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-prometheus#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "prom-client": "^11.1.3"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.12.0",
    "egg-bin": "^4.9.0",
    "egg-mock": "^3.20.1",
    "egg-rpc-base": "^1.1.0",
    "egg-rpc-generator": "^1.1.0",
    "eslint": "^5.8.0",
    "eslint-config-egg": "^7.1.0",
    "mz-modules": "^2.1.0",
    "urllib": "^2.31.1",
    "webstorm-disable-index": "^1.2.0"
  }
}
