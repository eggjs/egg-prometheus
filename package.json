{
  "name": "egg-prometheus",
  "version": "1.0.2",
  "description": "Prometheus plugin for Egg.js",
  "main": "app.js",
  "eggPlugin": {
    "name": "prometheus",
    "optionalDependencies": [
      "rpc"
    ]
  },
  "files": [
    "app.js",
    "lib",
    "app",
    "config",
    "agent.js"
  ],
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "sh test/init.sh && TEST_TIMEOUT=10000 egg-bin cov",
    "lint": "eslint . --fix",
    "ci": "npm run autod -- --check && egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg-prometheus.git"
  },
  "keywords": [
    "egg",
    "prometheus",
    "metrics"
  ],
  "author": "gxcsoccer <gxcsoccer@126.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-prometheus#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "await-first": "^1.0.0",
    "cluster-client": "^3.0.1",
    "connection": "^1.0.0",
    "mz-modules": "^2.1.0",
    "prom-client": "^11.3.0",
    "sdk-base": "^3.5.1"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "await-event": "^2.1.0",
    "egg": "^2.21.1",
    "egg-bin": "^4.12.3",
    "egg-mock": "^3.22.2",
    "egg-rpc-base": "^1.3.1",
    "egg-rpc-generator": "^1.3.1",
    "eslint": "^5.16.0",
    "eslint-config-egg": "^7.3.1",
    "pedding": "^1.1.0",
    "urllib": "^2.33.3",
    "webstorm-disable-index": "^1.2.0"
  }
}
